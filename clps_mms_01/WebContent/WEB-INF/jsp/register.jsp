<body class="loginbody">
<div class="dataEye">
	<div class="loginbox registbox">
		<div class="logo-a">
			<a href="login.jsp" title="CLPS办公室管理系统">
				<img src="images/clps_logo.png">
			</a>
		</div>
		<div class="login-content reg-content">
			<div class="loginbox-title">
				<h3 align="center">用户注册</h3>
			</div>
			<form id="signupForm" action="registerServlet" method="post">
			<div class="login-error"></div>
			<div class="row">
				<label class="field" for="username" >用户名</label>
				<input type="text"  class="input-text-user noPic input-click" name="username" id="username">
			</div>
			<div class="row">
				<label class="field" for="password">密码</label>
				<input type="password"  class="input-text-password noPic input-click" name="password" id="password">
			</div>
			<div class="row">
				<label class="field" for="passwordAgain">确认密码</label>
				<input type="password" value="" class="input-text-password noPic input-click" name="passwordAgain" id="passwordAgain">
			</div>
			<div class="row">
				<label class="field" for="email">注册邮箱</label>
				<input type="text"  class="input-text-user noPic input-click" name="email" id="email">
			</div>
			<div class="row">
				<label class="field" for="nickname">你的昵称</label>
				<input type="text"  class="input-text-user noPic input-click" name="nickname" id="nickname">
			</div>
			<div class="row">
				<label class="field" for="tel">手机号码</label>
				<input type="text"  class="input-text-user noPic input-click" name="tel" id="tel">
			</div>
			<div class="row tips">
				<input type="checkbox" id="checkBox">
				<label>
				我已阅读并同意
				<a href="#" target="_blank">隐私政策、服务条款</a>
				</label>
			</div>
			<div class="row btnArea" align="right">
				<!-- <a class="login-btn" id="submit">注册</a> -->
			<input type="submit" align="right" value="注册"/>
			</div>
			</form>
		</div>
		<div class="go-regist">
			已有帐号,请<a href="#" class="link">登录</a>
		</div>
	</div>
	
