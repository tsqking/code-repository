public class Comment implements Serializable {

	/**
	 * serialVersionUID:TODO(用一句话描述这个变量表示什么).
	 * 
	 * @since JDK 1.8
	 */
	private static final long serialVersionUID = 1L;
	private int c_id;
	private String c_send_nickname;
	private String c_rec_nickname;
	private String c_date;
	private String c_check_nickname;
	private String c_check_date;
	private String w_id;
	private String c_body;
	private String c_is_enable;

	public Comment() {
		super();
	}

	public Comment(int c_id, String c_send_nickname, String c_rec_nickname, String c_date, String c_check_nickname,
			String c_check_date, String w_id, String c_body, String c_is_enable) {
		super();
		this.c_id = c_id;
		this.c_send_nickname = c_send_nickname;
		this.c_rec_nickname = c_rec_nickname;
		this.c_date = c_date;
		this.c_check_nickname = c_check_nickname;
		this.c_check_date = c_check_date;
		this.w_id = w_id;
		this.c_body = c_body;
		this.c_is_enable = c_is_enable;
	}

	public int getC_id() {
		return c_id;
	}

	public String getC_send_nickname() {
		return c_send_nickname;
	}

	public String getC_rec_nickname() {
		return c_rec_nickname;
	}

	public String getC_date() {
		return c_date;
	}

	public String getC_check_nickname() {
		return c_check_nickname;
	}

	public String getC_check_date() {
		return c_check_date;
	}

	public String getW_id() {
		return w_id;
	}

	public String getC_body() {
		return c_body;
	}

	public String getC_is_enable() {
		return c_is_enable;
	}

	public void setC_id(int c_id) {
		this.c_id = c_id;
	}

	public void setC_send_nickname(String c_send_nickname) {
		this.c_send_nickname = c_send_nickname;
	}

	public void setC_rec_nickname(String c_rec_nickname) {
		this.c_rec_nickname = c_rec_nickname;
	}

	public void setC_date(String c_date) {
		this.c_date = c_date;
	}

	public void setC_check_nickname(String c_check_nickname) {
		this.c_check_nickname = c_check_nickname;
	}

	public void setC_check_date(String c_check_date) {
		this.c_check_date = c_check_date;
	}

	public void setW_id(String w_id) {
		this.w_id = w_id;
	}

	public void setC_body(String c_body) {
		this.c_body = c_body;
	}

	public void setC_is_enable(String c_is_enable) {
		this.c_is_enable = c_is_enable;
	}

	@Override
	public String toString() {
		return "Comment [c_id=" + c_id + ", c_send_nickname=" + c_send_nickname + ", c_rec_nickname=" + c_rec_nickname
				+ ", c_date=" + c_date + ", c_check_nickname=" + c_check_nickname + ", c_check_date=" + c_check_date
				+ ", w_id=" + w_id + ", c_body=" + c_body + ", c_is_enable=" + c_is_enable + "]";
	}

}
